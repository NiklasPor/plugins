{
  "name": "renamer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:renamer": "vite ./src/renamer",
    "dev:find_and_replace": "vite ./src/find_and_replace",
    "build:renamer": "vite build ./src/renamer --outDir ../../dist/renamer --emptyOutDir",
    "build:find_and_replace": "vite build ./src/find_and_replace --outDir ../../dist/find_and_replace --emptyOutDir",
    "check": "svelte-check --tsconfig ./tsconfig.json && tsc -p tsconfig.node.json",
    "format": "prettier --check '**/*.{js,json,jsx,md,ts,svelte,css}' --write",
    "pack": "npx framer-plugin-tools@latest pack",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^4.0.0-next.4",
    "@tsconfig/svelte": "^5.0.4",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.6",
    "svelte": "^5.0.0-next.201",
    "svelte-check": "^3.8.4",
    "tslib": "^2.6.3",
    "typescript": "^5.2.2",
    "vite": "^5",
    "vite-plugin-framer": "^1.0.1",
    "vite-plugin-mkcert": "^1"
  },
  "dependencies": {
    "framer-plugin": "^2.0.4"
  }
}
